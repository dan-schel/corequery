{
  "name": "corequery",
  "version": "0.1.8",
  "type": "module",
  "files": [
    "server/dist",
    "web/dist",
    "scripts/generate-pwa-assets/dist"
  ],
  "exports": "./server/dist/server/index.js",
  "types": "./server/dist/server/index.d.ts",
  "bin": {
    "corequery-generate-pwa-assets": "./scripts/generate-pwa-assets/bin"
  },
  "scripts": {
    "build": "scripts/build",
    "setup": "scripts/setup",
    "dev": "scripts/dev",
    "start": "scripts/start",
    "test": "vitest --run",
    "test-coverage": "vitest --run --coverage",
    "lint": "scripts/lint",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "bump-check": "bump-check --ignore \"^renovate\\/\""
  },
  "dependencies": {
    "@dan-schel/js-utils": "4.5.5",
    "express": "5.2.1",
    "node-html-parser": "7.0.1",
    "sharp": "0.34.5",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@preact/preset-vite": "2.10.2",
    "@types/express": "5.0.6",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "4.0.16",
    "chalk": "5.6.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "preact": "10.27.2",
    "preact-iso": "2.11.0",
    "prettier": "3.7.4",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "typescript-eslint": "8.51.0",
    "vite": "7.2.4",
    "vite-plugin-pwa": "1.2.0",
    "vitest": "4.0.16"
  },
  "author": "dan-schel",
  "description": "A platform for building web apps serving transit information.",
  "homepage": "https://github.com/dan-schel/corequery",
